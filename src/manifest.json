{
  "name": "gerrit tree",
  "version": "0.0.2",
  "manifest_version": 3,
  "author": "Hacode",
  "description": "Chrome extension to display Gitlab code in tree format",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "imgs/gitlab.png",
    "48": "imgs/gitlab.png",
    "128": "imgs/gitlab.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["libs/jquery/jquery.js",
             "libs/jstree/jstree.js",
             "libs/pjax/jquery.pjax.js",
             "libs/highlight/highlight.min.js",
             "libs/nprogress/nprogress.js",
             "libs/base64-js/base64js.min.js",
             "gitlabtree.js"],
      "css": ["main.css",
              "libs/nprogress/nprogress.css",
              "libs/jstree/jstree.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["libs/Font-Awesome/css/all.min.css",
    "libs/Font-Awesome/webfonts/*"],
      "matches": ["<all_urls>"]
    }
  ]
}