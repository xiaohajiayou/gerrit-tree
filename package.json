{
  "name": "gerrit-tree",
  "version": "1.5.0",
  "description": "Chrome extension to display gerrit code in tree format. Useful for developers who frequently read source in gerrit and do not want to download or checkout too many repositories.",
  "directories": {
    "doc": "docs",
    "test": "test",
    "src": "src"
  },
  "scripts": {
    "start": "gulp build",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrankFan/gerrit-tree.git"
  },
  "keywords": [
    "git",
    "gerrit",
    "tree",
    "gerrit tree"
  ],
  "overrides": {
    "graceful-fs": "^4.2.11"
  },
  "author": "fy98.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FrankFan/gerrit-tree/issues"
  },
  "homepage": "https://github.com/FrankFan/gerrit-tree",
  "devDependencies": {
    "del": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.5.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stats": "^0.0.3",
    "gulp-uglify": "^2.0.0",
    "mocha": "^5.0.4"
  }
}
